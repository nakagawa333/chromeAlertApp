{
    "name": "眼精疲労対策タイマー",
    "description": "しっかりと休憩を取って、眼精疲労から眼を守りましょう",
    "version": "0.1.0",
    "manifest_version":3,
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "background": {
        "service_worker": "./Background/background.js",
        "type":"module"
    },

    "action":{
        "default_popup":"./popup.html"
    },

    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs",
        "alarms"
    ],
    "options_page":"./options.html"
}